# Get official giturl for scripts
(cd ${ZDHOME}/ZonamaDev/;curl -s "http://downloads.zonamaserver.org/zonamadev/giturl.txt" --data-urlencode "x-zdupdate=rc.fasttrack" --data-urlencode "x-uuid=$(/usr/sbin/dmidecode -s system-uuid)" --data-urlencode "x-version=$(cat /.swgemudev.version)" --data-urlencode "x-zd-version=$(git describe --tag)" --data-urlencode "x-zd-repo=$(git ls-remote --get-url)" --data-urlencode "x-zd-branch=$(git rev-parse --abbrev-ref HEAD)" --data-urlencode "x-id=$(cat /var/lib/dbus/machine-id)") 2> /dev/null > /.swgemudev.giturl
